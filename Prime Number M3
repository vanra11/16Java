import java.util.Scanner;
class PrimeM3
{
	public static void main(String args[])	
	{
	int count;
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter upper limit: ");
	int u=sc.nextInt();
    System.out.println("Prime numbers between 1 and limit: ");
	for(int j=1;j<=u;j++)
	{
	    count=0;
	    for(int i=1;i<=j;i++)
    	{
    	   if(j%i==0)
    	   {
    	        count++;        
    	   }
    	}
	    if(count==2)
	       System.out.print(j+";");     
	}
	}
}
