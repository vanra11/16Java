import java.util.Scanner;
import java.io.*;
class Main {
	
	public static void main(String args[]) {
		int sum=0;
		Scanner sc= new Scanner(System.in);
		int num=sc.nextInt();
		for (int i=0;  i<num; i++)
		{
			if (i%2==0)
			{
				System.out.println(i);
				sum+=i;
			}
		}
		System.out.println("Sum=" +sum);
	}
}
