import java.util.Scanner;
class Reverse
{
  public static void main(String [] args)
  {
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter Num: ");
    int n=sc.nextInt();
    int rev=0;
    int r;
    int temp;
    temp=n;
    while(n>0)
    {
      r=n%10;
      rev=(rev*10)+r;
      n=n/10;
    }
    System.out.println("Reverse: "+rev);
  }
}
